#include <iostream>
#include <string>
using namespace std;

int main() {
    string a = "Hai";
    string b = "Rafi";

    // append()
    string hasil1 = a;
    hasil1.append(b);

    // operator +
    string hasil2 = a + b;

    cout << "append(): " << hasil1 << endl;
    cout << "+ operator: " << hasil2 << endl;
}
